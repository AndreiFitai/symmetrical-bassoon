{
  "env": {
      "es2021": true,
      "node": true,
      "jest/globals": true
  },
  "extends": [
      "standard",
      "eslint-config-prettier",
      "plugin:jest/all"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "eslint-plugin-prettier",
      "jest"
  ],
  "rules": {
    "prettier/prettier": "error"
  },
  "globals": {
    "Express":true,
    "NodeJS":true
  }
}
